{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2020",
    "sourceMap": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext", "WebWorker"],
    "strict": true,
    "strictPropertyInitialization": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "alwaysStrict": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node"
  },
  "exclude": ["node_modules"]
}
